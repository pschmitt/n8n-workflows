      {
        "parameters": {
          "language": "pythonNative",
          "pythonCode": "import re\n\nDEBUG = False\n\npayload = [item[\"json\"] for item in _items]\n\nemoji_mappings = [\n  x\n  for x in payload\n  if isinstance(x, dict) and x.get(\"emoji\") and x.get(\"regex\")\n]\n\ntask_data = [\n  x\n  for x in payload\n  if isinstance(x, dict) and x.get(\"id\") and x.get(\"content\")\n]\n\nchanged_tasks = []\nllm_tasks = []\n\nfor task in task_data:\n  content = task.get(\"content\", \"\")\n  matched = False\n\n  for em in emoji_mappings:\n    emoji = em.get(\"emoji\")\n    re_pattern = em.get(\"regex\")\n\n    if not emoji or not re_pattern:\n      continue\n\n    if DEBUG:\n      print(f\"content: '{content}' - emoji: '{emoji}' - re: '{re_pattern}'\")\n\n    if re.match(re_pattern, content, re.IGNORECASE):\n      task[\"content\"] = f\"{emoji} {content}\"\n      changed_tasks.append(task)\n      matched = True\n      break\n\n  if not matched:\n    llm_tasks.append(task)\n\nreturn [\n  {\n    \"json\": {\n      \"changed_tasks\": changed_tasks,\n      \"llm_tasks\": llm_tasks,\n    }\n  }\n]"
        },
